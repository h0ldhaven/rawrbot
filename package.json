  "version": "1.0.0",
  "main": "build/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "deploy": "ts-node-dev --transpile-only src/scripts/deploy-commands.ts",
    "build": "npm run type-check && npm run esbuild",
    "type-check": "tsc --project tsconfig.json --noEmit",
    "esbuild": "esbuild src/index.ts --bundle --platform=node --target=node24 --format=cjs --minify --outfile=build/bot.js",
    "lint": "eslint src/**/*.{ts,js}",
    "start": "node build/index.js"
  },
  "type": "commonjs",
  "dependencies": {
    "@discordjs/rest": "^2.6.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.2",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "chalk": "^5.6.2",
    "dayjs": "^1.11.19",
    "discord-api-types": "^0.38.37",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "esbuild": "^0.27.1",
    "eslint": "^9.39.2",
    "globals": "^16.5.0",
    "jiti": "^2.6.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0"
  }
}
